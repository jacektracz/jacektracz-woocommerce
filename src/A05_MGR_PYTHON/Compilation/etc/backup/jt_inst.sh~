#!/bin/bash

export KREDITNETHOME=/home/developer/hdd/MGR/CMR
export KREDITNET_CONFIG_PATH=/home/developer/hdd/MGR/CMR/config
export KREDITNETLOG=/home/developer/hdd/MGR/CMR/log
export FREETDS=/home/developer/hdd/MGR/CMR/3rdparty/freetds
export FREETDSCONF=/home/developer/hdd/MGR/CMR/config/freetds.conf
export ARCH=x86Linux

export LD_LIBRARY_PATH=${FREETDS}/lib/${ARCH}:$LD_LIBRARY_PATH
export PATH=${FREETDS}/bin/${ARCH}:$PATH

